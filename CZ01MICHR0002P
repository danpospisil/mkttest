/system scheduler add interval=1w name=weekly_backup on-event="/system script run backup_to_ftp" policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon start-date=jul/28/2017 start-time=23:23:00
/system scheduler add name=schedule1 policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon start-date=jul/25/2017 start-time=09:18:29
/system scheduler add interval=5s name=IPSec-keep-alive on-event="# DrMax Chodov\r\
    \n/ping 172.16.4.201 src-address=10.224.0.3 count=1\r\
    \n# ViaPharma Chodov\r\
    \n/ping 10.214.7.161 src-address=10.224.0.3 count=1\r\
    \n# PL\r\
    \n/ping 10.17.0.1 src-address=10.224.0.3 count=1\r\
    \n# RS\r\
    \n/ping 10.50.0.1 src-address=10.224.0.3 count=1" policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon start-date=dec/31/2017 start-time=00:00:00
/system script add dont-require-permissions=no name=backup_to_ftp owner=twada policy=ftp,read,write,policy,test,password,sniff,sensitive source="###### ftp vars\r\
    \n:local ftpserver (\"172.16.4.61\");\r\
    \n:local ftpuser (\"mkt_backup\");\r\
    \n:local ftppass (\"Pukcab_Tkm\");\r\
    \n:local dstpath (\"/share_Cloud/01-Dr.MAX-Net/BKP_configs/\");\r\
    \n\r\
    \n###### e-mail vars\r\
    \n:local smtpserver (\"172.16.4.104\");\r\
    \n:local smtpport (\"25\");\r\
    \n\r\
    \n###### months array \r\
    \n:local months (\"jan\",\"feb\",\"mar\",\"apr\",\"may\",\"jun\",\"jul\",\"aug\",\"sep\",\"oct\",\"nov\",\"dec\");\r\
    \n\r\
    \n###### get time\r\
    \n:local ts [/system clock get time]\r\
    \n:set ts ([:pick \$ts 0 2].[:pick \$ts 3 5].[:pick \$ts 6 8])\r\
    \n\r\
    \n###### get Date\r\
    \n:local ds [/system clock get date]\r\
    \n\r\
    \n###### convert name of month to number\r\
    \n:local month [ :pick \$ds 0 3 ];\r\
    \n:local mm ([ :find \$months \$month -1 ] + 1);\r\
    \n:if (\$mm < 10) do={ :set mm (\"0\" . \$mm); }\r\
    \n\r\
    \n###### set \$ds to format YYYY-MM-DD\r\
    \n:set ds ([:pick \$ds 7 11] . \$mm . [:pick \$ds 4 6])\r\
    \n\r\
    \n###### get system name\r\
    \n:local sysname [/system identity get name];\r\
    \n\r\
    \n###### create filename\r\
    \n:local bkpfilename ([\$sysname].\"-\".[\$ds].\"-\".[\$ts].\".backup\");\r\
    \n:local rscfilename ([\$sysname].\"-\".[\$ds].\"-\".[\$ts].\".backup.rsc\");\r\
    \n\r\
    \n###### create backup file\r\
    \n/system backup save name=\$bkpfilename;\r\
    \n/export compact file=\$rscfilename;\r\
    \n\r\
    \n###### upload file via FTP\r\
    \n/tool fetch address=\$ftpserver src-path=\$bkpfilename user=\$ftpuser mode=ftp password=\$ftppass dst-path=(\$dstpath.\$bkpfilename) upload=yes ;\r\
    \n/tool fetch address=\$ftpserver src-path=\$rscfilename user=\$ftpuser mode=ftp password=\$ftppass dst-path=(\$dstpath.\$rscfilename) upload=yes ;\r\
    \n\r\
    \n###### delay before deleting file from local filesystem\r\
    \n:delay 10s;\r\
    \n\r\
    \n###### delete file\r\
    \n/file remove \$bkpfilename;\r\
    \n/file remove \$rscfilename;\r\
    \n\r\
    \n###### add log record\r\
    \n/log info \"scheduled weekly backup completed\";\r\
    \n\r\
    \n###### send notification email\r\
    \n/tool e-mail send server=\$smtpserver start-tls=yes port=\$smtpport from=\"<\$sysname@dr-max.global>\" to=\"tvaruzek@drmax.cz\" subject=\"\$sysname: weekly backup completed\" body=\"router weekly backup completed. Backup archive uploaded to \$ftpserver.\\r\\nArchive location \$dstpath\$bkpfilename .\"\r\
    \n/log info \"notification email to your_email_address sent\";"
/tool bandwidth-server set authenticate=no enabled=no
/tool e-mail set from="" start-tls=yes
/tool graphing interface add
/tool graphing resource add
/tool netwatch add down-script="/log info \"SK DNS DOWN\"" host=10.7.14.195 up-script="/log info \"SK DNS UP\""
/tool sniffer set file-limit=10000KiB file-name=private.pcap filter-interface=vlan819-DCx-VPNHUBS-DIRECT-INET filter-ip-protocol=gre filter-operator-between-entries=and memory-limit=10000KiB